{
  "name": "tiles-api",
  "version": "1.0.0",
  "description": "Api for tiles client",
  "main": "server.js",
  "scripts": {
    "start": "node index.js",
    "dev-docker": "../node_modules/nodemon/bin/nodemon.js --debug=5858",
    "dev-host": "nodemon --debug=5858",
    "start-watch": "nodemon index.js --inspect=0.0.0.0:9229",
    "start-wait-debuger": "nodemon index.js --inspect-brk=0.0.0.0:9229",
    "test": "cross-env NODE_ENV=test PORT=8081 mocha --timeout 10000 --exit --inspect=0.0.0.0:9230",
    "test-watch": "nodemon --exec \"npm test\"",
    "test-wait-debuger": "cross-env NODE_ENV=test PORT=8081 mocha --no-timeouts --exit --inspect-brk=0.0.0.0:9230"
  },
  "author": "Brandon Sorgdrager",
  "repository": {
    "type": "git",
    "url": "git://github.com/bsord/tiles-api.git"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongoose": "^5.4.13",
    "pm2": "^2.10.4",
    "shortid": "^2.2.14",
    "socket.io": "^2.1.1",
    "socket.io-redis": "^5.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.1.4",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.8"
  }
}
